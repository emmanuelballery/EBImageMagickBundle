<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="eb_imagemagick.imagemagick.class">EB\ImageMagickBundle\ImageMagick</parameter>
    </parameters>
    <services>
        <service id="eb_imagemagick.imagemagick" class="%eb_imagemagick.eb_imagemagick.class%">
            <argument>%eb_imagemagick.command.convert%</argument>
            <argument id="logger" type="service"/>
        </service>
    </services>
</container>